class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        l,r = 0,1
        b  = 0
        
        for r in range(len(prices)):
            if prices[l]>prices[r]:
                l = r
            m = prices[r] - prices[l]
            if b < m:
                b = m
        
        if b!= 0:
            
               
            return b
        else:
            return 0
